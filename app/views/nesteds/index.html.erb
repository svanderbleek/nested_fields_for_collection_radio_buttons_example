<h1>Listing nesteds</h1>

<table>
  <thead>
    <tr>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @nesteds.each do |nested| %>
      <tr>
        <td><%= link_to 'Show', nested %></td>
        <td><%= link_to 'Edit', edit_nested_path(nested) %></td>
        <td><%= link_to 'Destroy', nested, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

